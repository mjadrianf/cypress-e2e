pipeline {
   agent any

   stages {
       stage('Dependencies') {
           steps {
               sh 'npm i'
               sh 'npm install cypress-testrail'
           }
       }
       stage('e2e Tests') {
           steps {
               sh 'npm run cy:run'
           }
       }
       stage('Deploy') {
           steps {
               echo 'Deploying....'
           }
       }
   }
}
